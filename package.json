{
  "name": "burger-queen",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest --coverage --env=jsdom",
    "fake-api": "json-server --watch src/assets/products.json",
    "orders-api": "json-server --watch src/assets/orders.json --port 3001",
    "auth-api": "json-server-auth src/assets/auth.json --port 3004",
    "all": "npm run fake-api && npm run orders-api && npm auth-api && npm run dev"
  },
  "dependencies": {
    "bootstrap": "^5.3.0-alpha1",
    "eslint": "^8.32.0",
    "json-server": "^0.17.1",
    "json-server-auth": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.42.1",
    "react-router": "^6.7.0",
    "react-router-dom": "^6.8.1"
  },
  "devDependencies": {
    "@swc/core": "^1.3.35",
    "@swc/jest": "^0.2.24",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "babel-jest": "^29.4.2",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.4.2",
    "jest-environment-jsdom": "^29.4.2",
    "standard": "^17.0.0",
    "vite": "^4.0.0"
  },
  "eslintConfig": {
    "extends": "./node_modules/standard/eslintrc.json"
  }
}
